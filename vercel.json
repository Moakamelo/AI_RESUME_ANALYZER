{
    "version": 2,
    "builds": [
        {
            "src": "backend/app/main.py",
            "use": "@vercel/python",
            "config": {
                "pythonVersion": "3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/app/main.py"
        }
    ]
}